[branch]
  autosetupmerge = true
[commit]
  gpgsign = true
	template = .git-message
[credential]
	helper = cache --timeout=3600
[gpg]
	program = signify-notes
[tag]
	gpgsign = true
	forceSignAnnotated = true
[remote "upstream"]
	url = https://github.com/BegleyBrothers/cookbook-ignite.git
	fetch = +refs/heads/*:refs/remotes/upstream/*
