[branch]
  autosetupmerge = true
[commit]
  gpgsign = true
	template = .git-message
[credential]
	helper = cache --timeout=3600
[gpg]
	program = signify-notes
[tag]
	gpgsign = true
	forceSignAnnotated = true
[remote "origin"]
	url = https://github.com/bbros-dev/creditrisk.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[user]
	signingKey = ~/.config/signify/begleybrothers@gmail.com.sec
	name = Begley Brothers Inc.
	email = begleybrothers@gmail.com
